{
  "compilerOptions": {
    "target": "es2020",
    "declaration": true,
    "module": "ES2020",
    "moduleResolution": "Node",
    "removeComments": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true
    
  }
}
